#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,num,power,result=1;
    printf("Enter a number: ");
    scanf("%d",&num);
    printf("Enter the power: ");
    scanf("%d",&power);

    i=0;
    while(i<power)
    {
        result=result*num;
        i++;
    }
    printf("Result:  %d",result);
    return 0;
}
